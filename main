--Key System

local keys = {        --as you can see very private only ten keys wow
	"3919848564",
	"1790549218",
	"1891118795",
	"6712426901",
	"0272308042",
	"2494688377",
	"4042499043",
	"8197283449",
	"8566991816",
	"0694111794",
	"1787989288",
}

local counter = 1
local keyCheck
for i,v in pairs(keys) do
	if counter == #keys then
		--not whitelisted!
		keys = ""
		game.Players.LocalPlayer:Kick("Not a valid key!")
	else
		if v == _G.Key then
			--Whitelisted!
			print("Successfully whitelisted!")
			keyCheck = _G.Key
			keys = ""
		else
			counter = counter +1
		end
	end
end

while true do
	if _G.Key == keyCheck then

	else
		game.Players.LocalPlayer:Kick("nigger isnt whitelisted LOL")
	end
	wait()
end

